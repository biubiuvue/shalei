/* bar */

.bar {
  width: 620rpx;
  height: 92rpx;
  border-radius: 10rpx;
  background-color: rgba(0, 0, 0, 0.5);
  position: relative;
  top: 110rpx;
  left: 18rpx;
  padding: 38rpx 46rpx;
}

.bar .barLeft {
  width: 40rpx;
