  width: 150rpx;
  position: absolute;
  right: -52rpx;
}

/* 进度条 */

.barLine {
  width: 460rpx;
  height: 10rpx;
  padding: 0;
  position: relative;
  left: 80rpx;
  top: 28rpx;
}

.barLine .barLineLeft {
  height: 10rpx;
  width: 189rpx;
  background-color: #268be5;
  border-radius: 5rpx 0 0 5rpx;
}

