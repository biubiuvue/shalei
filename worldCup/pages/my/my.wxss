    height: 100%;
}

.noData view {
    position: relative;
    top: 509rpx;
    color: #fff;
    text-align: center;
    font-size: 36rpx;
    z-index: 1000;
}

.noData image {
    position: absolute;
    top: 230rpx;
    width: 177rpx;
    height: 223rpx;
}

.noData .woman1 {
    left: 72rpx;
}

.noData .woman2 {
    left: 290rpx;
}

.noData .woman3 {
