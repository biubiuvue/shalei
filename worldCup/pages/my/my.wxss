.bg {
    position: fixed;
    width: 100%;
    /* height: auto; */
}

.listContent {
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0rpx;
    left: 0;
}

.list {
    width: 700rpx;
    height: 264rpx;
    background-color: rgba(0, 52, 0, 0.6);
    border-radius: 30rpx;
    margin-bottom: 28rpx;
    position: relative;
    left: 25rpx;
    top: 31rpx;
    overflow: hidden;
}

.shadow {
    width: 700rpx;
    height: 264rpx;
    background: #000;
    opacity: 0.6;
    position: absolute;
    top: 0;
    border-radius: 30rpx;
}

.opacity {
    opacity: 0.8;
}

.list .head {
    width: 700rpx;
    height: 74rpx;
    background-color: rgba(0, 22, 8, 0.57);
}

.list .head .result {
    color: #fcc442;
    font-size: 36rpx;
    line-height: 74rpx;
    position: absolute;
    left: 38rpx;
}

.list .head .result1 {
    color: #fff;
    font-size: 36rpx;
    line-height: 74rpx;
    position: absolute;
    left: 38rpx;
}

.list .head .time {
    color: #fff;
    font-size: 30rpx;
    line-height: 74rpx;
    position: absolute;
    right: 37rpx;
    font-weight: 100;
}

.list .cont {
    width: 700rpx;
    color: #fff;
}

.list .cont .left {
    width: 120rpx;
    position: absolute;
    top: 102rpx;
    left: 73rpx;
}

.list .cont .left image {
    width: 120rpx;
    height: 90rpx;
}

.list .cont .left view {
    color: #fff;
    font-size: 28rpx;
    line-height: 38rpx;
    text-align: center;
    font-weight: 100;
    width: 180rpx;
    position: absolute;
    left: -30rpx;
}

.list .cont .right {
    width: 120rpx;
    position: absolute;
    top: 102rpx;
    right: 73rpx;
}

.list .cont .right image {
    width: 120rpx;
    height: 90rpx;
}

.list .cont .right view {
    color: #fff;
    font-size: 28rpx;
    line-height: 38rpx;
    text-align: center;
    font-weight: 100;
    width: 180rpx;
    position: absolute;
    right: -30rpx;
}

.list .cont .center {
    width: 160rpx;
    height: 150rpx;
    text-align: center;
    position: absolute;
    left: 270rpx;
    top: 100rpx;
}

.list .cont .center .top {
    color: #fff;
    font-size: 30rpx;
    line-height: 40rpx;
    height: 40rpx;
    letter-spacing: 3rpx;
    font-weight: 400;
}

.list .cont .center .bot {
    text-align: center;
    color: #fff812;
    letter-spacing: 21rpx;
    font-size: 56rpx;
    padding-left: 14rpx;
}
.nav{
    position: fixed;
    top: 0rpx;
